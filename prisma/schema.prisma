generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model Chat {
  id           String        @id @default(uuid(7))
  createdAt    DateTime      @default(now())
  satisfaction Satisfaction?
  messages     Json?
  carImage     Json?
}

enum Satisfaction {
  VERY_BAD
  BAD
  NORMAL
  GOOD
  VERY_GOOD
}
